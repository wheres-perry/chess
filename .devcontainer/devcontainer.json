{
    "name": "Java with MySQL",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspaces/chess",
    "customizations": {
        "vscode": {
            "extensions": [
                "vscjava.vscode-java-pack",
                "cweijan.vscode-mysql-client2",
                "vscjava.vscode-java-test"
            ]
        }
    },
    "forwardPorts": [
        3306
    ],
    "postCreateCommand": "git config --global --add safe.directory /workspaces/chess"
}